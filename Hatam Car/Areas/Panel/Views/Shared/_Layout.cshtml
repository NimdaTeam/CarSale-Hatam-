<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title> کنترل پنل</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Areas/Panel/dist/css/bootstrap-theme.css">
    <!-- Bootstrap rtl -->
    <link rel="stylesheet" href="~/Areas/Panel/dist/css/rtl.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Areas/Panel/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Areas/Panel/bower_components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Areas/Panel/dist/css/AdminLTE.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Areas/Panel/dist/css/skins/_all-skins.min.css">
   
    <link href="~/Assets/DropzoneAssets/dropzone.css" rel="stylesheet" />

    <link href="~/Assets/DropzoneAssets/ng-dropzone.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<!-- ADD THE CLASS fixed TO GET A FIXED HEADER AND SIDEBAR LAYOUT -->
<!-- the fixed layout is not compatible with sidebar-mini -->
<body class="hold-transition skin-blue fixed sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="../../index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">پنل</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>کنترل پنل</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>


                <input type="hidden" value="@Session["Sess_UserID"] " id="txtUserID" />

                <input type="hidden" value="@Session["Sess_UserName"]" id="txtEmail" />

                <input type="hidden" value="@Session["Sess_Name"]" id="txtName" />

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <!-- Notifications: style can be found in dropdown.less -->
                        <!-- Tasks: style can be found in dropdown.less -->
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="ion ion-ios-person" style="color:white"></span>
                                <span class="hidden-xs">@Session["Sess_Name"]</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Areas/Panel/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                                    <p>
                                        @Session["Sess_Name"]

                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat">پروفایل</a>
                                    </div>
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">خروج</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>
        <!-- right side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-right col-md-1">
                        <span class="ion ion-ios-person" style="color:white"></span>
                    </div>
                    <div class="pull-right info col-md-1">
                        <p>@Session["Sess_Name"]</p>

                    </div>
                </div>
                <!-- search form -->
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">منو</li>
                    <li class="active">
                        <a href="/Panel/PanelHome">
                            <i class="fa fa-dashboard"></i> <span>داشبورد</span>

                        </a>

                    </li>
                    <li>
                        <a href="/Panel/Ticket_Adding">
                            <i class="fa fa-edit"></i> <span>ثبت آگهی</span>
                        </a>

                    </li>

                    <li>
                        <a href="#">
                            <i class="fa fa-th"></i> <span>آگهی های من</span>
                            <span class="pull-left-container">

                            </span>
                        </a>
                    </li>
                    <li><a href="#"><i class="ion ion-android-options"></i> <span>تنظیمات حساب کاربری</span></a></li>
                    <li id="Users"><a href="#"><i class="ion ion-person-stalker"></i> <span>کابران سایت</span></a></li>
                    <li id="TicketManage"><a href="#"><i class="fa fa-ticket" ng-if="@Session["Sess_UserRole"] = UserAddi"></i> <span>مدیریت آگهی ها</span></a></li>


                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>


        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            <!-- Main content -->
            <section class="content">
                <div ng-app="MyApp1" ng-controller="MyCtrl1">
                    @RenderBody()
                </div>

            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        <footer class="main-footer text-right">
            <strong>تمامی حقوق این وبسایت متعلق به  <a href="/Home">WispoCar</a> است </strong>
        </footer>


        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <script>
  
         
        alert(sessionStorage.UserID);
        sessionStorage.UserRole = @Session["Sess_UserRole"];
        sessionStorage.UserName=@Session["Sess_Name"];
    
       
 
    </script>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Areas/Panel/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Areas/Panel/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <!-- SlimScroll -->
    <script src="~/Areas/Panel/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/Panel/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Areas/Panel/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Areas/Panel/dist/js/demo.js"></script>
    <script src="~/Assets/DropzoneAssets/dropzone.js"></script>
    <script src="~/Assets/DropzoneAssets/ng-dropzone.min.js"></script>



    @RenderSection("DeclareJS", false)
</body>
</html>
